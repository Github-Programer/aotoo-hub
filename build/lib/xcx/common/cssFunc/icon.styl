@font-face {
  font-family: 'iconfont';  /* project id 1074841 */
  src: url('//at.alicdn.com/t/font_1074841_0yce7tfy03c.eot');
  src: url('//at.alicdn.com/t/font_1074841_0yce7tfy03c.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1074841_0yce7tfy03c.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1074841_0yce7tfy03c.woff') format('woff'),
  url('//at.alicdn.com/t/font_1074841_0yce7tfy03c.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1074841_0yce7tfy03c.svg#iconfont') format('svg');
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}


iconfont()
  +cache()
    font-family:"iconfont" !important
    font-style:normal
    -webkit-font-smoothing: antialiased
    -webkit-text-stroke-width: 0.2px
    -moz-osx-font-smoothing: grayscale
    // font-size: 16px

content(n)
  if (n != '')
    iconfont()
  content: n