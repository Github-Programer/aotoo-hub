@import "../../common/commonfunc"
.announcement
  height 100%
  .btn-primary.btn-small
    width: 326rpx;
    line-height: 56rpx;
    font-size: 28rpx;
  .card-notification
    width: auto
    height: auto
    margin-bottom: 20rpx
    .htitles
      border: 2rpx solid transparent;
      border-radius: 10rpx;
      border-color:#ccaf90;
      background-color:#fff;
      display flex
      flex-direction: column
      min-height 130rpx
      height auto
      width:  calc(100% - 4rpx)
      overflow hidden
      .t-item
        padding:15rpx;
        &:first-child
          font-size 25rpx
          height 20%
        &:last-child
          padding-left 20rpx
          padding-top 0
          flex 1
      .btn-small
        margin-left 10px
        padding: 0
.modal-head-index
  height: 100rpx
  line-height: @height
  box-shadow: 0px 10px 12px 0px rgba(21,61,106,0.03)
  font-size: 40rpx
  padding: 0 24rpx;
  background: #fff;
  position: relative;
  z-index: 11;
  color: #333
.modal-body-index
  height: calc(100% - 152rpx)
  padding: 24rpx
  overflow: hidden

.closeIt
  z-index: 99;


.index-modal
  width: 80vw
  height: 66vh

.index-swiper-modal
  background: #fff
  border-radius: $modal-radius
  width: 80vw
  height: auto
  color: #fff
  margin-bottom: 60rpx
  overflow: hidden
  &.item-hei
    height: 66vh
  .swiper-item
    border-radius: $modal-radius
    overflow: hidden
  .item-wrapper
    // ss-display-flex(column, center, center, center)
    text-align: center
    border-radius: $modal-radius
    overflow: hidden
  .item-header
    padding: 3.5vh 0 5vh;
  .item-body
    display: inline-flex
    margin-bottom: 3vh;

.item-wrapper-1
  background: url('data:image/png;base64,') no-repeat #fff
  background-position: top center
  background-size: contain;
.item-wrapper-2
  background: url('data:image/png;base64,') no-repeat #fff
  background-position: top center
  background-size: contain;
  

.wx-swiper-dots
  bottom: 4vh!important

.index-header
  background: url("data:image/png;base64,") no-repeat #f4f4f4
  background-position: top center
  background-size: contain;


.index-btn
  ss-btnFunc( $btn-radius, $bgc: #2567FF, $bc: #2567FF, $color: #fff, $hover-bg: #2567FF, $border-radius-small: $btn-radius-s)
  border: none
  box-shadow: $box-shadow-btn
  background: linear-gradient(90deg,rgba(87,142,254,1),rgba(37,103,255,1))
  width: 172rpx
  line-height: 60rpx
  font-weight: bold
  &:active
    background: #2567FF
    border-color: #2567FF
    color: #fff

.ss-tree-list
  padding: 40rpx 25rpx