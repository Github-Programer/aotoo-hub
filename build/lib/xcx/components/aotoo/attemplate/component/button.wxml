<template name="button">
  <button 
    size="{{item.size||'default'}}"
    type="{{item.type||'default'}}"
    plain="{{item.plain||false}}"
    disabled="{{item.disabled||false}}"
    loading="{{item.loading||false}}"
    form-type="{{item['form-type']||''}}"
    open-type="{{item['open-type']||''}}"
    hover-class="{{item['hover-class']||'button-hover'}}"
    hover-stop-propagation="{{item['hover-stop-propagation']||false}}"
    hover-start-time="{{item['hover-start-time']||20}}"
    hover-stay-time="{{item['hover-stay-time']||70}}"
    lang="{{item['lang']||'en'}}"
    bindgetuserinfo="{{item['bindgetuserinfo']}}"
    session-from="{{item['session-from']||''}}"
    send-message-title="{{item['send-message-title']||'当前标题'}}"
    send-message-path="{{item['send-message-path']||'当前分享路径'}}"
    send-message-img="{{item['send-message-img']||'截图'}}"
    show-message-card="{{item['show-message-card']||false}}"
    bindcontact="{{item['bindcontact']||''}}"
    bindgetphonenumber="{{item['bindgetphonenumber']||''}}"
    app-parameter="{{item['app-parameter']||''}}"
    binderror="{{item['binderror']||''}}"
    bindopensetting="{{item['bindopensetting']||''}}"
    class="{{item.class||''}}"
    style="{{item['style']||''}}"
  >
    {{item.title}}
  </button>
</template>